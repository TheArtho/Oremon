        "priority": 0,
        "probability": 1,
        "angle_of_view_horizontal": 90,
        "angle_of_view_vertical": 90
      },
      "minecraft:behavior.float": { "priority": 0 },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0,
