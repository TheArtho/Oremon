      },
      "oremon:make_female": {
        "set_property": {
          "oremon:gender": "female"
        }
      },
      "oremon:make_genderless": {
        "set_property": {
          "oremon:gender": "genderless"
        }
      },
      "oremon:sit": {
        "add": { "component_groups": ["sit"] }
      },
      "oremon:tame": {
        "add": { "component_groups": ["tamed"] }
      },
      "oremon:evolve_ivysaur": {
        "add": { "component_groups": ["evolve_to:ivysaur"] }
