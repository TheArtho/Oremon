          "priority": 4,
          "speed_multiplier": 1.2,
          "start_distance": 5,
          "stop_distance": 3
        }
      },
      "sit": {
        "minecraft:sittable": {}
      },
      "evolve_to:ivysaur": {
        "minecraft:transformation": {
          "into": "oremon:ivysaur<oremon:evolution>",
          "keep_owner": true
        }
      }
    },
    "components": {
